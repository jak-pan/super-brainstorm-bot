{
  "name": "superbrainstormbot",
  "version": "1.0.0",
  "description": "Multi-AI collaborative brainstorming bot for Discord with Notion integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src --ext .ts",
    "lint:mermaid": "node scripts/lint-mermaid.js",
    "lint:scripts": "eslint scripts --ext .ts,.js",
    "lint:all": "npm run lint && npm run lint:mermaid && npm run lint:scripts",
    "type-check": "tsc --noEmit",
    "precommit": "npm run lint:all",
    "prepare": "husky || true"
  },
  "keywords": [
    "discord",
    "bot",
    "ai",
    "chatgpt",
    "claude",
    "grok",
    "notion",
    "brainstorming"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^5.3.0",
    "@openrouter/ai-sdk-provider": "^1.2.1",
    "ai": "^5.0.89",
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3",
    "p-queue": "^9.0.0",
    "tiktoken": "^1.0.22",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "openai": "^4.76.1",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
