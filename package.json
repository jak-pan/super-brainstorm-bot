{
  "name": "superbrainstormbot",
  "version": "1.0.0",
  "description": "Multi-AI collaborative brainstorming bot for Discord with Notion integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src --ext .ts",
    "lint:mermaid": "node scripts/lint-mermaid.js",
    "lint:scripts": "eslint scripts --ext .ts,.js",
    "lint:all": "npm run lint && npm run lint:mermaid && npm run lint:scripts",
    "update-models": "tsx scripts/update-models.ts",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "precommit": "npm run lint:all",
    "prepare": "husky || true"
  },
  "keywords": [
    "discord",
    "bot",
    "ai",
    "chatgpt",
    "claude",
    "grok",
    "notion",
    "brainstorming"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/xai": "^2.0.31",
    "@anthropic-ai/sdk": "^0.68.0",
    "@notionhq/client": "^2.2.15",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "openai": "^4.28.0",
    "p-queue": "^8.0.1",
    "tiktoken": "^1.0.10",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
